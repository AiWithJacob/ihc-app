{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(?!api/)(?!audit-log-diagnostics\\.html$).*",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/google-calendar/sync-deleted",
      "schedule": "0 4 * * *"
    }
  ]
}
